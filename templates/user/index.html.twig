{% extends 'base.html.twig' %}



{% block body %}
<h1>Inscription</h1>
<table class="table table-striped table-bordered">
  <thead>
  <tr>
    <th>lastname</th>
    <th>firstname</th>
    <th>email</th>
    <th>country</th>
    <th>role</th>
  </tr>
  </thead>
  <tbody>
  {% for user in users %}
  <tr>
  <td>{{user.lastname}}</td>
  <td>{{user.firstname}}</td>
  <td>{{user.email}}</td>
  <td>{{user.country.name}}</td>
  <td>{{user.role.name}}</td>

  </tr>
  {% endfor %}
</tbody>
</table>
{% endblock %}
